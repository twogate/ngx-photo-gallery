    "@angular/compiler-cli": "^18.0.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "2.29.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.1",
    "ng-packagr": "^18.0.0",
    "postcss": "^8.4.20",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
