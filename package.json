    "@angular/compiler-cli": "^14.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "^28.1.3",
    "ng-packagr": "^14.1.0",
    "postcss": "^8.2.15",
    "prettier": "^2.7.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^20.0.0",
